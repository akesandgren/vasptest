System = CeO2 : fcc
ISTART = 0
ICHARG = 2
PREC=Normal
ENCUT = 400.00
EDIFF = 0.1E-04
LREAL=Auto
LCHARG = .FALSE.
LWAVE = .FALSE.
LDAU = .TRUE. #Switch on the L(S)DA+U
LDAUTYPE = 2  #Dudarev's approach
LDAUL = 3 -1  #Ce and O LDAUL, LDAUU and LDAUJ must be specified for all atomic species
LDAUU = 5.5 0 #Ce and O
LDAUJ = 0 0   #Ce and O
LMAXMIX=6 
ISMEAR=0
SIGMA=0.1
NCORE=1
